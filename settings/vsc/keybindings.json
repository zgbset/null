// 将按键绑定配置放入此文件中即可覆盖默认值
[
    {
        "key": "ctrl+b",
        "command": "workbench.action.tasks.runTask",
        "args": "SingleBuild"
    },
    {
        "key": "ctrl+l",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+k",
        "command": "-editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+b",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    }
]
